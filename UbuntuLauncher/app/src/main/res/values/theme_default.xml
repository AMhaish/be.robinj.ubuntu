<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Launcher -->
    <bool name="theme_default_launcher_background_dynamic">true</bool>
    <drawable name="theme_default_launcher_background">@android:color/transparent</drawable>
    <integer name="theme_default_launcher_bfb_location">@integer/position_top</integer>
    <drawable name="theme_default_launcher_bfb_image">@drawable/launcher_bfb</drawable>
    <integer name="theme_default_launcher_preferences_location">@integer/position_bottom</integer>
    <drawable name="theme_default_launcher_preferences_image">@drawable/launcher_preferences</drawable>
    <bool name="theme_default_launcher_applauncher_backgroundcolour_dynamic">true</bool>
    <color name="theme_default_launcher_applauncher_backgroundcolour">@android:color/transparent</color>
    <drawable name="theme_default_launcher_applauncher_background">@drawable/widget_launcher_applauncher_background</drawable>
    <drawable name="theme_default_launcher_applauncher_gradient">@drawable/launcher_icon_bg</drawable>
    <drawable name="theme_default_launcher_applauncher_running">@drawable/launcher_app_running</drawable>

    <!-- Panel -->
    <integer name="theme_default_panel_location">@integer/position_top</integer>
    <dimen name="theme_default_panel_height">28dp</dimen>
    <drawable name="theme_default_panel_background">@drawable/panel_background</drawable>
    <integer name="theme_default_panel_close_location">@integer/position_left</integer>
    <drawable name="theme_default_panel_close_image">@drawable/dash_close</drawable>
    <integer name="theme_default_panel_preferences_location">@integer/position_right</integer>
    <drawable name="theme_default_panel_preferences_image">@drawable/panel_cog</drawable>

    <!-- Dash -->
    <drawable name="theme_default_dash_background_gradient">@drawable/dash_background_gradient</drawable>
</resources>